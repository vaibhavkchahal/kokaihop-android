<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">
    <data></data>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:gravity="center|bottom"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingBottom="@dimen/feed_tab_padding_bottom"
            android:paddingEnd="@dimen/feed_tab_padding_right"
            android:paddingStart="@dimen/feed_tab_padding_left"
            android:paddingTop="@dimen/feed_tab_padding_top"
            android:textColor="@color/grey_FF8E8E93"
            android:textSize="@dimen/feed_tab_text_small"
            app:font="@{`SS-ProRegular`}"/>

        <View android:layout_width="@dimen/divider_height"
              android:layout_height="match_parent"
              android:background="@color/grey_EBEBEB"/>

    </LinearLayout>

</layout>
